<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" prefHeight="165.0" prefWidth="239.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EntryDialog">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <TextField fx:id="heightField" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <TextField fx:id="widthField" GridPane.columnIndex="3" GridPane.rowIndex="3" />
      <Label text="Height" GridPane.rowIndex="1" />
      <Label text="Width" GridPane.rowIndex="3" />
      <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="173.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
         <children>
            <Label fx:id="errorLabel" prefHeight="47.0" prefWidth="140.0" textFill="RED" wrapText="true" />
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#createTerrainMap" onKeyPressed="#createTerrainMap" prefHeight="36.0" prefWidth="45.0" text="OK" />
         </children>
      </HBox>
      <Label text="Choose the terrain map size" GridPane.columnIndex="3" />
   </children>
</GridPane>
